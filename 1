def is_correct(brackets: list[str]) -> bool:
    a = list()
    for i in brackets:
        if i == '(':
            a.append(i)
        elif i == ')' and len(a) != 0:
            if a[-1] == '(':
                a.pop()
    if len(brackets) % 2 == 1:
        return False
    if len(a) == 0:
        return True
    else:
        return False
print(is_correct(['(', ')', '(', ')']))
